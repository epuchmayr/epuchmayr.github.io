function linkify(a){a=a.replace(/(https?:\/\/([\w\d_\-=&\/|+#~]|[\.,:\?](?!\s|$))+)/ig,"<a href='$1' rel='nofollow' target='_blank'>$1</a>");a=a.replace(/@([\w\d_]+)/g,"@<a href='http://twitter.com/$1' rel='nofollow' target='_blank'>$1</a>");return a=a.replace(/(\s|^|:)(#)([^!\s\.,&#\-]+)/g,"$1<a href='http://search.twitter.com/search?q=%23$3' rel='nofollow' target='_blank'>$2$3</a>")}generateLatestTweets("div._stream","hootsuite_help",2);
